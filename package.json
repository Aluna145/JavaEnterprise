		"axios": "^0.18.0",
		"babel-loader": "^8.0.2",
		"clean-webpack-plugin": "^0.1.19",
		"css-loader": "^1.0.0",
		"express": "^4.16.3",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-test-renderer": "^16.5.2",
		"style-loader": "^0.23.0",
		"webpack": "^4.19.0",
		"webpack-cli": "^3.1.0",
		"redux-mock-store": "^1.5.3",
