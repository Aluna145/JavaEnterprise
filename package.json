	"main": "src/index.jsx",
	"scripts": {
		"dev": "nodemon ./startServer.js",
		"dev:new": "webpack-serve",
		"build": "webpack -p --progress",
		"prod": "webpack --mode production",
		"test": "jest",
		"lint": "eslint --ext .js,.jsx ./src"
	},
	"keywords": [
